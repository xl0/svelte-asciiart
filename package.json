{
	"name": "svelte-asciiart-demo",
	"private": true,
	"version": "0.0.1",
	"readme": "../../README.md",
	"workspaces": [
		"packages/*"
	],
	"dependencies": {
		"svelte-asciiart": "workspace:*"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "bun --cwd=packages/svelte-asciiart run prepack && vite build",
		"build:demo": "bun --cwd=packages/svelte-asciiart run prepack && BASE_PATH=/svelte-asciiart vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"test": "bun --cwd=packages/svelte-asciiart run test",
		"test:run": "bun --cwd=packages/svelte-asciiart run test"
	},
	"type": "module",
	"devDependencies": {
		"@internationalized/date": "^3.10.1",
		"@lucide/svelte": "^0.561.0",
		"@shikijs/rehype": "^3.20.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/package": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/svelte": "^5.3.0",
		"@types/hast": "^3.0.4",
		"@types/mdast": "^4.0.4",
		"bits-ui": "^2.14.4",
		"clsx": "^2.1.1",
		"jsdom": "^27.3.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"publint": "^0.3.16",
		"rehype-stringify": "^10.0.1",
		"remark": "^15.0.1",
		"remark-gfm": "^4.0.1",
		"remark-html": "^16.0.1",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.2",
		"shiki": "^3.20.0",
		"svelte": "^5.46.0",
		"svelte-check": "^4.3.5",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.18",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"unified": "^11.0.5",
		"vite": "^7.3.0",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^4.0.16"
	},
	"keywords": [
		"svelte",
		"ascii-art",
		"svg"
	]
}
